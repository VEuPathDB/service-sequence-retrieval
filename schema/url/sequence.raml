#%RAML 1.0 Library


uses:
  common: ../common.raml

types:
  SequencePostRequest:
    additionalProperties: false
    properties:
      features:
        type: array
        items: common.Feature
      deflineFormat: common.DeflineFormat
      forceStrandedness:
        type: boolean
        default: false
      basesPerLine:
        type: integer
        default: 60

  SequencePostResponse: common.PlainTextFastaResponse
